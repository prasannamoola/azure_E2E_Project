{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfEtoEProject"
		},
		"ls_AzureDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLake'"
		},
		"ls_azuresql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresql'"
		},
		"ls_AzureDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagazureprojecte2e.dfs.core.windows.net/"
		},
		"ls_azuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "e2eprojectserver.database.windows.net"
		},
		"ls_azuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "e2eDB"
		},
		"ls_azuresql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pmadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfEtoEProject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLake_accountKey": {
						"value": "[parameters('ls_AzureDataLake_accountKey')]"
					},
					"ls_azuresql_password": {
						"value": "[parameters('ls_azuresql_password')]"
					},
					"ls_AzureDataLake_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLake_properties_typeProperties_url')]"
					},
					"ls_azuresql_properties_typeProperties_server": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_server')]"
					},
					"ls_azuresql_properties_typeProperties_database": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_database')]"
					},
					"ls_azuresql_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}